{
  "name": "@kb/ember-cli-file-saver",
  "version": "2.0.2",
  "description": "Provides file saving capability to an Ember CLI application.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "homepage": "https://github.com/cogniteev/ember-cli-file-saver",
  "repository": {
    "type": "git",
    "url": "git://github.com/cogniteev/ember-cli-file-saver"
  },
  "bugs": {
    "url": "https://github.com/cogniteev/ember-cli-file-saver/issues"
  },
  "author": "Cogniteev <techteam@cogniteev.com>",
  "license": "MIT",
  "dependencies": {
    "ember-cli-babel": "^7.26.6",
    "file-saver": "2.0.5"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.11.3",
    "ember-ajax": "^5.1.1",
    "ember-cli": "~3.28.4",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-qunit": "^4.0.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.28.0",
    "loader.js": "^4.7.0"
  },
  "keywords": [
    "ember-addon",
    "FileSaver",
    "save",
    "file",
    "arraybuffer",
    "blob"
  ],
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
